    <div class="tab-pane @if (Model != null) { <text>active </text> }" id="results-tab">
        <!-- all results go in here -->
        <div class="viewing-count linearbg">
            <span id="viewing-span">Viewing: </span><span id="min-view-count">1</span> - <span id="total-view-count">
                <span id="max-view-count">3</span>
                of @if (Model != null)
                { @Model.Count();
                }
            </span>
            <!-- sorting controls -->
            <span id="sorting-control-span">
                Sort:
                <select id="sorting-select">
                    @Html.Raw(ViewBag.SortOptionList)
                </select>
            </span>

        </div>
        <div id="inner-results-container">
            @try
            {
                var count = 1;

                foreach (var item in Model)
                {

                    var imageUrls = (List<string>)ViewData[item.ListingID + "_url"];
                    var imageUrlThumbs = (List<string>)ViewData[item.ListingID + "_thumb"];
                    count += 1;
                    var displayPrice = "";
                    var sortPrice = 0;
                    
                    if (item.ListPrice.IndexOf(".") > 0)
                                {
                                    sortPrice = int.Parse(item.ListPrice.Substring(0, item.ListPrice.IndexOf(".")));
                                    displayPrice = String.Format("{0:C0}", sortPrice);
                                }
                                else if (item.ClosePrice != "") {
                                    sortPrice = int.Parse(item.ClosePrice.Substring(0, item.ClosePrice.IndexOf(".")));
                                    displayPrice = String.Format("{0:C0}", sortPrice );
                                    
                                }
                                else
                                {
                                    sortPrice = 0;
                                    displayPrice = "<span class='no-price'>Price Unknown</span>";
                                }
                   
                    <div class="results-property linearbg" title="@sortPrice-@item.Beds-@item.BathsFull-@item.LotAreaAcre-@item.NetSQFT-@item.PropertyAge"  id="@item.ListingID" data-lat="@item.Latitude" data-lng="@item.Longitude">
                        <div>
                            <div class="results-address">@item.FullStreetAddress.ToLower()</div>
                            <span class="results-city">@item.CityName.ToLower()</span><span class="results-price badge badge-primary">
                                @displayPrice
                            </span>
                        </div>
                        <div style="display:inline-block; width:274px;">
                            <div class="has-more-pics">
                                <div id="scroller-div" style="width:4000px;position:relative;">
                                    @if (imageUrls.Count < 1)
                                    {
                                        <span style="float:left;">
                                            <img src='../../Content/themes/metro/assets/img/no_image.jpg' width=120 height=120 />
                                        </span>

                                    }
                                    @for (var i = 0; i < imageUrlThumbs.Count; i++)
                                    {
                                        <span style="float:left;">
                                            <a class="fancybox" rel="@item.ListingID" href='@imageUrls[i]'>
                                                @if (item.LocaleListingStatus == "SOLD")
                                                { <img class="img-overlay" src='../../Content/themes/metro/assets/img/sold.png' width='120' height='120'> }
                                                else if (item.LocaleListingStatus == "PENDING") { 
                                                    <img class="img-overlay" src='../../Content/themes/metro/assets/img/pending.png' width='120' height='120'>  
                                                }                                                                                                          
                                                <img src='@imageUrlThumbs[i]' width=120 height=120 />
                                            </a>
                                        </span>
                                    }
                                    

                                </div>
                            </div>
                            <br>
                            @if (imageUrlThumbs.Count >= 1) { 
                            <div class="image-scroll-bg">
                                <img id="images-down" class="arrow" src="../../Content/themes/metro/assets/img/white-left-arrow.gif" /><span id="image-count" data-imgcount="@imageUrlThumbs.Count" data-distance="144" data-currentimg="1">1 of @imageUrlThumbs.Count</span>
                                <img id="images-up" class="arrow" src="../../Content/themes/metro/assets/img/white-right-arrow.gif" />
                            </div>
                            }
                            <span class="results-property-info" style="margin-top:-112px;">
                                <span class="property-spans">
                                    <span class="results-property-beds">@item.Beds BEDS</span>
                                    <table class="details-table">
                                        <tr><td class="tl"><span class="tdspanl">SQ FT.<br />@if (item.NetSQFT.Length > 0) { @String.Format(item.NetSQFT.Substring(0, item.NetSQFT.Length - 2)) } else { @String.Format("0") }</span></td><td class="tr"><span class="tdspanr">ACRES<br />@item.LotAreaAcre</span></td></tr>
                                        <tr><td class="bl"><span class="tdspanl">BATHS<br />@item.BathsFull </span></td><td class="br"><span class="tdspanr">YEARS<br />@item.PropertyAge<br /></span></td></tr>
                                    </table>
                                    <a href="#" class="badge badge-success results-detail-btn linearbg-dark" style="font-size:12px;height:18px;font-weight:bold;">Show Details</a>
                                </span>
                            </span>
                        </div>
                    </div>
                }

            }
            catch (NullReferenceException) { }
        </div>  <!--/inner-results-container-->
    </div>
</div>
</div>

