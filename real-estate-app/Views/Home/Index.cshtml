			, infoBoxClearance: new google.maps.Size(1, 1)
			, isHidden: false
			, pane: "floatPane"
			, enableEventPropagation: false
	        };
            */

	        // detect where the marker is on the map
	        // if closer than some number of pixels to each edge of map canvas, set 
	        // pixeloffset: 
	        // midpoints: height: 256 px
            //            width: 332 px
            // 

	        var pos = getPixelPosition(marker, map);
	        var offsetX = pos.x < 332 ? 30 : -294;
	        var offsetY = pos.y < 256 ? 100 : 20;

	        var bubbleClass;

	        if (offsetX == 30 && offsetY == 100)
	            bubbleClass = "bubbleq1";
	        else if (offsetX == 30 && offsetY == 20)
	            bubbleClass = "bubbleq3";
	        else if (offsetX == -294 && offsetY == 20)
	            bubbleClass = "bubbleq4";
	        else 
	            bubbleClass = "bubbleq2";


	        var infoWindow = new InfoBox({
